
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Clínica Belman</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("customStyles", required: false)
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,400;0,500;0,600;0,700;0,800;1,600;1,700&display=swap" rel="stylesheet">

    <style>
        body {
            margin: 0;
            font-family:"Poppins", sans-serif;
        }

        .navbar {
            background-color: #63b2ff;
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 10px;
        }

            .navbar .container {
                display: flex;
                align-items: center;
                justify-content: center;
                width: 100%;
                max-width: 1200px;
            }

        .navbar-brand {
            font-size: 1.5rem;
            font-weight: bold;
            color: #fff;
            left: 100px
            /*          display: flex;
            align-items: center;*/
        }

            .navbar-brand img {
                width: 30px;
                margin-right: 8px;
            }

        .navbar-nav {
            display: inline-flex;
            gap: 20px;
            background-color: #b3d4fc;
            padding: 5px 20px;
            border-radius: 25px;
        }

            .navbar-nav li {
                list-style-type: none;
            }

            .navbar-nav .nav-link {
                text-decoration: none;
                color: #000;
                font-size: 1rem;
                font-weight: bold;
                padding: 8px 16px;
                border-radius: 20px;
                transition: background-color 0.3s, color 0.3s;
                display: inline-block; /* Asegura que cada enlace esté en línea */
            }

                .navbar-nav .nav-link.active {
                    background-color: #fff;
                    color: #007bff;
                }

                .navbar-nav .nav-link:hover {
                    background-color: #e0f0ff;
                    color: #007bff;
                }

        .search-icon {
            font-size: 1.2rem;
            color: #000;
            margin-left: 15px;
        }
    </style>
</head>
<body>
    @{
        var currentController = ViewContext.RouteData.Values["controller"] as string;
        var currentAction = ViewContext.RouteData.Values["action"] as string;
    }
    <nav class="navbar">
        <div class="container ">
            <a class="navbar-brand position-absolute" href="@Url.Action("Index", "Home")">
                <svg width="162" height="44" viewBox="0 0 162 44" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect width="162" height="44" fill="url(#pattern0_303_367)" />
                    <defs>
                        <pattern id="pattern0_303_367" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_303_367" transform="matrix(0.00183517 0 0 0.00675676 -0.120287 0)" />
                        </pattern>
                        <image id="image0_303_367" width="676" height="148" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
            </a>
            <ul class="navbar-nav d-flex flex-row">
                <li>
                    @Html.ActionLink("Inicio", "Index", "Home", null, new
                    {
                        @class = "nav-link " + (currentController == "Home" && currentAction == "Index" ? "active" : "")
                    })
                </li>
                <li>
                    @Html.ActionLink("Pacientes", "ListaPacientes", "GestionarPacientes", null, new
                    {
                        @class = "nav-link " + (currentController == "GestionarPacientes" && currentAction == "ListaPacientes" ? "active" : "")
                    })
                </li>
                <li>
                    @Html.ActionLink("Citas", "GestionCita", "GestionarCitas", null, new
                   {
                       @class = "nav-link " + (currentController == "GestionarCitas" && currentAction == "GestionCita" ? "active" : "")
                   })
                </li>
                <li>
                    @Html.ActionLink("Acerca de", "About", "Home", null, new
                    {
                        @class = "nav-link " + (currentController == "Home" && currentAction == "About" ? "active" : "")
                    })
                </li>
                <li>
                    @Html.ActionLink("Consultas", "Contact", "Home", null, new
                    {
                        @class = "nav-link " + (currentController == "Home" && currentAction == "Contact" ? "active" : "")
                    })
                </li>

                <svg width="56" height="33" viewBox="0 0 56 33" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M32.5475 20.5475L36.73 24.73" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                    <path d="M20 15.17C20 19.1299 23.2101 22.34 27.17 22.34C29.1533 22.34 30.9487 21.5347 32.2467 20.2332C33.5403 18.9363 34.34 17.1465 34.34 15.17C34.34 11.2101 31.1299 8 27.17 8C23.2101 8 20 11.2101 20 15.17Z" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                </svg>

            </ul>
        </div>

        <svg width="56" height="33" viewBox="0 0 56 33" fill="none" xmlns="http://www.w3.org/2000/svg">
            <path d="M32.5475 20.5475L36.73 24.73" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
            <path d="M20 15.17C20 19.1299 23.2101 22.34 27.17 22.34C29.1533 22.34 30.9487 21.5347 32.2467 20.2332C33.5403 18.9363 34.34 17.1465 34.34 15.17C34.34 11.2101 31.1299 8 27.17 8C23.2101 8 20 11.2101 20 15.17Z" stroke="black" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
        </svg>

        </ul>
        </div>
    </nav>

    <div class=" container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Clínica Belman</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/bootstrap.bundle.js"></script>
    @Scripts.Render("~/bundles/complementos")
    @RenderSection("scripts", required: false)
</body>
</html>